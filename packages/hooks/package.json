{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest --coverage --verbose --color",
    "test:watch": "jest --coverage --verbose --color --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "setupFiles": [
      "../../setupTests"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@nanameue/hooks",
  "author": "bk",
  "module": "dist/hooks.esm.js",
  "size-limit": [
    {
      "path": "dist/hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "35f84dc36fa14c955fd4b06a430d6c66dcafd5ea"
}
